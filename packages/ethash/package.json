{
  "name": "@nomicfoundation/ethereumjs-ethash",
  "version": "3.0.4",
  "description": "An implementation of the Ethash consensus algorithm in JavaScript",
  "keywords": [
    "ethash",
    "ethereum",
    "POW"
  ],
  "homepage": "https://github.com/ethereumjs/ethereumjs-monorepo/tree/master/packages/ethash#readme",
  "bugs": {
    "url": "https://github.com/ethereumjs/ethereumjs-monorepo/issues?q=is%3Aissue+label%3A%22package%3A+ethash%22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ethereumjs/ethereumjs-monorepo.git"
  },
  "license": "MPL-2.0",
  "author": "mjbecze <mjbecze@gmail.com>",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "../../config/cli/ts-build.sh",
    "clean": "../../config/cli/clean-package.sh",
    "coverage": "../../config/cli/coverage.sh",
    "docs:build": "typedoc --options typedoc.cjs",
    "examples": "tsx ../../scripts/examples-runner.ts -- ethash",
    "lint": "../../config/cli/lint.sh",
    "lint:diff": "../../config/cli/lint-diff.sh",
    "lint:fix": "../../config/cli/lint-fix.sh",
    "test": "npx vitest run",
    "tsc": "../../config/cli/ts-compile.sh"
  },
  "dependencies": {
    "@nomicfoundation/ethereumjs-block": "https://gitpkg.now.sh/Amxx/ethereumjs-monorepo/packages/block?features/eip-5806-nomiclabs",
    "@nomicfoundation/ethereumjs-rlp": "https://gitpkg.now.sh/Amxx/ethereumjs-monorepo/packages/rlp?features/eip-5806-nomiclabs",
    "@nomicfoundation/ethereumjs-util": "https://gitpkg.now.sh/Amxx/ethereumjs-monorepo/packages/util?features/eip-5806-nomiclabs",
    "bigint-crypto-utils": "^3.2.2",
    "ethereum-cryptography": "0.1.3"
  },
  "devDependencies": {
    "@nomicfoundation/ethereumjs-common": "https://gitpkg.now.sh/Amxx/ethereumjs-monorepo/packages/common?features/eip-5806-nomiclabs"
