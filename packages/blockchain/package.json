{
  "name": "@nomicfoundation/ethereumjs-blockchain",
  "version": "7.0.4",
  "description": "A module to store and interact with blocks",
  "keywords": [
    "ethereum",
    "blockchain"
  ],
  "homepage": "https://github.com/ethereumjs/ethereumjs-monorepo/tree/master/packages/blockchain#readme",
  "bugs": {
    "url": "https://github.com/ethereumjs/ethereumjs-monorepo/issues?q=is%3Aissue+label%3A%22package%3A+blockchain%22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ethereumjs/ethereumjs-monorepo.git"
  },
  "license": "MPL-2.0",
  "author": "mjbecze <mjbecze@gmail.com>",
  "type": "commonjs",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "../../config/cli/ts-build.sh",
    "clean": "../../config/cli/clean-package.sh",
    "coverage": "DEBUG=ethjs npx vitest run --coverage.enabled --coverage.reporter=lcov",
    "docs:build": "typedoc --options typedoc.cjs",
    "examples": "tsx ../../scripts/examples-runner.ts -- blockchain",
    "examples:build": "npx embedme README.md",
    "lint": "../../config/cli/lint.sh",
    "lint:diff": "../../config/cli/lint-diff.sh",
    "lint:fix": "../../config/cli/lint-fix.sh",
    "test": "npm run test:node && npm run test:browser",
    "test:browser": "npx vitest run --browser.name=webkit --browser.provider=playwright --browser.headless",
    "test:node": "npx vitest run",
    "tsc": "../../config/cli/ts-compile.sh"
  },
  "dependencies": {
    "@nomicfoundation/ethereumjs-block": "https://gitpkg.now.sh/Amxx/ethereumjs-monorepo/packages/block?features/eip-5806-nomiclabs",
    "@nomicfoundation/ethereumjs-common": "https://gitpkg.now.sh/Amxx/ethereumjs-monorepo/packages/common?features/eip-5806-nomiclabs",
    "@nomicfoundation/ethereumjs-ethash": "https://gitpkg.now.sh/Amxx/ethereumjs-monorepo/packages/ethash?features/eip-5806-nomiclabs",
    "@nomicfoundation/ethereumjs-rlp": "https://gitpkg.now.sh/Amxx/ethereumjs-monorepo/packages/rlp?features/eip-5806-nomiclabs",
    "@nomicfoundation/ethereumjs-trie": "https://gitpkg.now.sh/Amxx/ethereumjs-monorepo/packages/trie?features/eip-5806-nomiclabs",
    "@nomicfoundation/ethereumjs-tx": "https://gitpkg.now.sh/Amxx/ethereumjs-monorepo/packages/tx?features/eip-5806-nomiclabs",
    "@nomicfoundation/ethereumjs-util": "https://gitpkg.now.sh/Amxx/ethereumjs-monorepo/packages/util?features/eip-5806-nomiclabs",
    "debug": "^4.3.3",
    "ethereum-cryptography": "0.1.3",
    "lru-cache": "^10.0.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18"
  }
}
